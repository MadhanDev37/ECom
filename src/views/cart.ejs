                <div class="address-container">
                    <h2>Shipping Address</h2>
                    <form id="cart-contact-form">
                        <div class="contact-details">
                            <h6>Contact Details</h6>
                            <div class="contact-details-container">
                                <div class="form-group">
                                    <input type="text" id="cart-firstname" placeholder="First Name">
                                    <span></span>
                                </div>
                                <div class="form-group">
                                    <input type="text" id="cart-lastname" placeholder="Last Name">
                                    <span></span>
                                </div>
                                <div class="form-group">
                                    <small>+91</small>
                                    <input type="number" id="cart-mobile" placeholder="Mobile" min="1">
                                    <span></span>
                                </div>
                            </div>
                            
                        </div>
                        <div class="address-details">
                            <h6>Address</h6>
                            <div class="form-group">
                                <textarea name="" id="cart-address" placeholder="Address" ></textarea>
                                <span></span>
                            </div>
                            <div class="form-group">
                                <input type="number" id="cart-pincode" placeholder="Pincode" min="1">
                                <span></span>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="preview-container">
                    <h2>Preview</h2>
                    <div class="preview-carts-list">
                    </div>
                </div>
            </div>
            <div class="cart-footer">
                <div class="carts active">
                    <div class="checkout">
                        <div class="carttotalItems">
                            <span>Total Items</span>
                            <span>Total Price</span>
                        </div>
                        <div class="cartprice">
                            <span class="ctiv"></span>
                            <span class="ctpv"></span>
                        </div>
                    </div>
                    <div class="ptc">
                        <button>Proceed to Checkout</button>
                    </div>
                </div>
                <div class="cart-address">
                        <button id="cart-back-btn">Back</button>
                        <button id="cart-proceed-btn">Proceed</button>
                </div>
                <div class="preview-section">
                    <div class="shipping-address">
                        <h6>Shipping Address</h6>
